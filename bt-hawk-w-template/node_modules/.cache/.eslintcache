ffer], { type: fileType });\r\n    FileSaver.saveAs(data, fileName + fileExtension);\r\n  }\r\n\r\n  // Generates PDF Report\r\n  pdf() {\r\n    var doc = new jsPDF('p', 'pt', 'a4');\r\n    doc.setLineWidth(2);\r\n    doc.text(200, 20, 'Sales Executive Stock Details');\r\n    doc.autoTable({\r\n      html: '#viewTable',\r\n      styles: {\r\n        overflow: 'linebreak',\r\n        fontSize: 7,\r\n        tableWidth: 900,\r\n        tableLineWidth: 0,\r\n        cellPadding: 1,\r\n        whiteSpace: 'noWrap',\r\n      },\r\n      margin: {\r\n        top: 40,\r\n        left: 15,\r\n        right: 15\r\n      }\r\n    });\r\n    doc.save('FSE_Stock_Detail.pdf');\r\n  }\r\n\r\n  // Getting Data from Backend API\r\n  getData(user_id) {\r\n    let dis_id = this.state.dis_id;\r\n    var response = $.ajax({\r\n      url: SERVER + '/user_stock_detail',\r\n      method: 'POST',\r\n      contentType: 'application/x-www-form-urlencoded',\r\n      async: false,\r\n      dataType: 'JSON',\r\n      data: {\r\n        dis_id: dis_id,\r\n        user_id: user_id\r\n      }\r\n    }).responseJSON;\r\n\r\n    if(response.status==200){\r\n      return response.data;\r\n    } else {\r\n      return [];\r\n    }\r\n  }\r\n\r\n  render() {\r\n    const { user_id, work_type } = this.props.location.state;\r\n    const data = this.getData(user_id);\r\n    const length = data.length;\r\n    //console.log(data);\r\n    return (\r\n      <div className=\"page-content-wrapper\">\r\n        <div className=\"page-content\">\r\n          <div className=\"page-bar\" style={{ marginBottom: '-20px' }}>\r\n            <ul className=\"page-breadcrumb\">\r\n              <li>\r\n                <Link to='/'>Home&nbsp;</Link>\r\n                <i className=\"fa fa-circle\"></i>\r\n              </li>\r\n              <li>\r\n                <span>&nbsp;Stock Detail </span>\r\n              </li>\r\n            </ul>\r\n            <div className=\"page-toolbar\">\r\n              <div>\r\n          